        {
          "id": 621524,
          "key": "055f296a-2a45-4124-8beb-b6e2b470b030",
          "title": "Tokenizing Words",
          "semantic_type": "Concept",
          "is_public": true,
          "user_state": {
            "node_key": "055f296a-2a45-4124-8beb-b6e2b470b030",
            "completed_at": null,
            "last_viewed_at": null,
            "unstructured": null
